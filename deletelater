import java.util.Scanner;

public class Main{
    
    public static void main(String[] args){
    	
    	int ans = 0;
    	String output = "";
    	Scanner keyb = new Scanner(System.in);
    	System.out.println("What is 2*(3+10)%3");
    	ans = keyb.nextInt();
    	
    	switch(ans) {
    	case 0: 
    		output = "Answer is 0";
    	case 1:
    		output = "Answer is 1";
    	case 2:
    		output = "Answer is 2";
    	case 4:
    		output = "Answer is 4";
    		
    	default:
    		output = "NO ANSWER";
    		
    	}
    	
    	System.out.println(output);
    	
    	
    	
    }

}
